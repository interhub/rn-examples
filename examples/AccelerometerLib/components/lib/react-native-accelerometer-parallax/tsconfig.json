{
  "compilerOptions": {
    "strict": true,
    "target": "ES2020",
    "lib": [
      "dom",
      "es6",
      "es2016.array.include",
      "es2017.object"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsx": "react-native",
    "outDir": "build",
    "declaration": true,
    "declarationDir": "declare"
  }
}
