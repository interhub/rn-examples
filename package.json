    "expo-media-library": "~12.1.2",
    "expo-permissions": "~12.1.1",
    "expo-sensors": "~10.2.2",
    "expo-splash-screen": "~0.11.2",
    "expo-status-bar": "~1.0.4",
    "expo-updates": "~0.8.1",
    "lodash": "^4.17.21",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "~0.63.4",
    "react-native-accelerometer-parallax": "^1.5.6",
    "react-native-code-push": "^7.0.1",
