    "@react-navigation/devtools": "^5.1.22",
    "@react-navigation/native": "^5.0.9",
    "@react-navigation/stack": "^5.1.1",
    "@types/clamp": "^1.0.0",
    "@types/lodash": "^4.14.171",
    "axios": "^0.21.1",
    "clamp": "^1.0.1",
    "expo": "^42.0.0",
    "expo-av": "~9.2.3",
